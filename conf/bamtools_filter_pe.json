{
    "filters" : [
        { "id" : "insert_neg_range",
          "insertSize" : ">=-200",
          "insertSize" : "<=-100"
        },

        { "id" : "insert_pos_range",
          "insertSize" : ">=100",
          "insertSize" : "<=200"
        },

        { "id" : "mismatch",
          "tag" : "XM:<=3"
        },

        { "id" : "cigar",
          "cigar" : "*S*"
        }
    ],

    "rule" : " ( insert_neg_range | insert_pos_range ) & mismatch & !cigar "

}
